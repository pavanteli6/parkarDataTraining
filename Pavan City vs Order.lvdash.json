{
  "datasets": [
    {
      "name": "a925fbe7",
      "displayName": "Customer Order Totals Grouped By State And City",
      "queryLines": [
        "SELECT\r\n",
        "  `state`,\r\n",
        "  `city`,\r\n",
        "  SUM(`total_orders`) AS total_orders\r\n",
        "FROM\r\n",
        "  `circuitbox_pavan`.`landingschema`.`gold_customer_order_summary`\r\n",
        "WHERE\r\n",
        "  `state` IS NOT NULL\r\n",
        "  AND `city` IS NOT NULL\r\n",
        "GROUP BY\r\n",
        "  `state`,\r\n",
        "  `city`\r\n",
        "ORDER BY\r\n",
        "  `state`,\r\n",
        "  `city`"
      ],
      "catalog": "newpavancatalog",
      "schema": "gold"
    }
  ],
